public interface  EmployeeDao{
	void saveEmployee(Person employee);
    
    List<Person> findAllEmployees();
     
    void deleteEmployeeBySsn(String ssn);
     
    Person findBySsn(String ssn);
     
    void updateEmployee(Person employee);
}